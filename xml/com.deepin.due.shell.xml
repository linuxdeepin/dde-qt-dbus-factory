<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN" "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
  <interface name="com.deepin.due.shell">
    <signal name="swipeDown">
    </signal>
    <signal name="swipeUp">
    </signal>
    <signal name="visibleChanged">
      <arg name="visible" type="b" direction="out"/>
    </signal>
    <signal name="rotationLockChanged">
      <arg name="lock" type="b" direction="out"/>
    </signal>
    <method name="Toggle">
    </method>
    <method name="Show">
    </method>
    <method name="Hide">
    </method>
    <method name="IsVisible">
      <arg type="b" direction="out"/>
    </method>
  </interface>
</node>
