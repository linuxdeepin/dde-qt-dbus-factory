<interface name="com.deepin.daemon.Authenticate.FaceVerify">
    <method name="Enroll">
        <arg name="name" type="s" direction="in"></arg>
    </method>
    <method name="Verify">
        <arg name="name" type="s" direction="in"></arg>
    </method>
    <method name="StopVerify"/>
    <method name="StopEnroll"/>
    <method name="DeleteFace">
        <arg name="name" type="s" direction="in"></arg>
    </method>
    <method name="GetFaceStatus">
        <arg name="name" type="s" direction="in"></arg>
        <arg name="isExist" type="b" direction="out"></arg>
    </method>
    <signal name="EnrollStatus">
        <arg name="name" type="s"></arg>
        <arg name="result" type="i"></arg>
        <arg name="msg" type="s"></arg>
    </signal>
    <signal name="VerifyStatus">
        <arg name="name" type="s"></arg>
        <arg name="result" type="i"></arg>
        <arg name="msg" type="s"></arg>
    </signal>
</interface>